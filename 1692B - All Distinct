#include <bits/stdc++.h>
using namespace std;

int main (){
    int t; cin >> t;
    while (t--){
        
        int n; cin >> n;
        int a[n];
        set<int> val;
        for (int i = 0; i < n; i++){
            cin >> a[i];
            val.insert(a[i]);
        }
       
        int size = val.size();
        if ((n - size) % 2 == 1){
            cout << size-1 << endl;
        }else{
            cout << size << endl;
        }
    }
}
