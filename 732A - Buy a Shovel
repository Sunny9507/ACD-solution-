#include <iostream>
using namespace std;
int main() {
    int k, r;
    cin >> k >> r;
    int i=2, cnt=1, value = k;
    while(true){
        if(value % 10 == 0){
            cout << cnt;
            break;
        }
        if(value % 10 == r){
            cout << cnt;
            break;
        }
        value = k*i;
        cnt++;
        i++;
    }
 
    return 0;
}
